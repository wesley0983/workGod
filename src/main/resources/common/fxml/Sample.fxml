<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.MainController">
<!--    <padding>-->
<!--        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />-->
<!--    </padding>-->


    <children>
        <MenuBar BorderPane.alignment="CENTER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Close" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About..." />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <SplitPane fx:id="mainSplitPanel" dividerPositions="0.20, 0.80" focusTraversable="false" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <items>
                <VBox prefHeight="200.0" prefWidth="100.0" spacing="4.0" BorderPane.alignment="CENTER">
                    <children>
                        <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="ALL_TABS" tabMinWidth="50.0" VBox.vgrow="ALWAYS">
                        </TabPane>
                    </children>
                </VBox>
            </items>
        </SplitPane>
        <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
        <TabPane tabClosingPolicy="UNAVAILABLE" GridPane.columnIndex="1">
            <tabs>
                <Tab text="廠商資訊">
                    <content>
                        <AnchorPane GridPane.columnIndex="0" GridPane.rowIndex="1">

                        </AnchorPane>
                    </content>
                </Tab>
                <Tab text="報表資訊">
                    <content>
                        <AnchorPane GridPane.columnIndex="0" GridPane.rowIndex="1">

                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        </HBox>
    </children>

</VBox>
